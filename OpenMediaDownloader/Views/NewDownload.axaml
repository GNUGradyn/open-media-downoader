<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:controls="clr-namespace:OpenMediaDownloader.Controls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OpenMediaDownloader.Views.NewDownload"
        Title="New Download"
        Height="800" Width="450"
        Background="#F0F0F0" >
    <StackPanel Name="Test">
        <Label FontSize="25" HorizontalAlignment="Center" Content="{Binding Title}" ToolTip.Tip="{Binding Title}"/>
        <Image ZIndex="1" Source="{Binding Thumbnail}" HorizontalAlignment="Stretch"></Image>
        <controls:ContentBox ZIndex="999"></controls:ContentBox>
    </StackPanel>
</Window>
